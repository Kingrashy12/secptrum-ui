{
  "presets": ["@babel/preset-env", "@babel/preset-react"],
  "plugins": [
    [
      "babel-plugin-styled-components",
      {
        "displayName": true, // Enables readable class names
        "namespace": "Sui", // Prefixes the class names with "Sui"
        "fileName": false // Optional: This prevents file paths from being included in the class name
      }
    ]
  ]
}
